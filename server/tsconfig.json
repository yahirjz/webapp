{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "resolveJsonModule": true,
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/**/*"
    ]
}